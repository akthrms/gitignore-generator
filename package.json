{
  "name": "gitignore-generator",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "./dist/index.js",
  "devDependencies": {
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.14",
    "@types/node": "13.13.21",
    "jest": "26.4.2",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "testMatch": [
      "./**/__tests__/*.spec.+(ts|js)"
    ]
  },
  "dependencies": {
    "inquirer": "^7.1.0"
  }
}
