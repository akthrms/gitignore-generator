{
  "name": "gitignore-generator",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "./dist/index.js",
  "devDependencies": {
    "@types/inquirer": "6.5.0",
    "@types/jest": "26.0.0",
    "@types/node": "13.13.11",
    "jest": "26.1.0",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "testMatch": [
      "./**/__tests__/*.spec.+(ts|js)"
    ]
  },
  "dependencies": {
    "inquirer": "^7.1.0"
  }
}
